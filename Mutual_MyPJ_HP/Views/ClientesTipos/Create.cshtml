@model Mutual_MyPJ_HP.Data.Entities.ClientesTipo

@{
    ViewData["Title"] = "Create";
}

<h4>Crear Cliente Tipo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClienteInae" class="control-label"></label>
                <input asp-for="ClienteInae" class="form-control" />
                <span asp-validation-for="ClienteInae" class="text-danger"></span>
            </div>
                        <br />
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-outline-primary btn-sm" />
                <a asp-action="Index" class="btn btn-outline-success btn-sm">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
